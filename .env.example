# Environment for local deploy
APP_NAME=E-Museu

APP_KEY=

# APP_ENV & APP_DEBUG configured in docker compose files but is important to keep here as well

# development values (always set these values before committing to test CI):

APP_ENV=local
APP_DEBUG=true
# or
# APP_ENV=staging
# APP_DEBUG=true


# local production values:

# APP_ENV=production
# APP_DEBUG=false

# Docker internal URL (container to container communication)
APP_URL=http://localhost:80

WEB_SERVER_PORT_LOCAL=9090
WEB_SERVER_PORT_PROD_LOCAL=9091

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

PHP_MYADMIN_PORT_LOCAL=9092

DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=user
DB_PASSWORD=user
DB_ROOT_PASSWORD=secret

# On staging and production, not local, set the DATABASE_URL in the environment (no db service).
# DATABASE_URL=mysql://user:your_password@db:3306/laravel

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_APP_NAME="${APP_NAME}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

STAGING_HTTP_USER=staging
STAGING_HTTP_PASSWORD=set_a_password

# Coolify environment example:

## Production:

# APP_DEBUG=false
# APP_ENV=production
# APP_KEY=please_generate_one
# APP_NAME=E-Museu
# APP_URL=http://emuseu.com
# DB_DATABASE=db_production
# DB_HOST=db-production
# DB_PASSWORD=password_production
# DB_ROOT_PASSWORD=password_root_production
# DB_USERNAME=emuseu_user


## Staging (HTTP Basic Auth: set STAGING_HTTP_USER and STAGING_HTTP_PASSWORD in Coolify app env):

# APP_DEBUG=false
# APP_ENV=staging
# APP_KEY=please_generate_one
# APP_NAME=E-Museu
# APP_URL=http://staging.emuseu.com
# STAGING_HTTP_USER=staging
# STAGING_HTTP_PASSWORD=set_a_password
# DB_DATABASE=emuseu_staging
# DB_HOST=db-staging
# DB_PASSWORD=password_staging
# DB_ROOT_PASSWORD=password_root_staging
# DB_USERNAME=emuseu_user